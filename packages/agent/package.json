{
  "name": "@ens-node-metadata/agent",
  "version": "0.2.7",
  "description": "CLI for registering AI agents on ENS using ERC-8004",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ens-agent": "./dist/cli.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "SKILL.md",
    "README.md",
    "examples"
  ],
  "scripts": {
    "version:patch": "pnpm version patch --no-git-tag-version",
    "version:minor": "pnpm version minor --no-git-tag-version",
    "agent": "bun src/cli.ts",
    "build": "tsup",
    "lint": "biome check ."
  },
  "dependencies": {
    "@ens-node-metadata/sdk": "workspace:*",
    "@ensdomains/ensjs": "^4.0.2",
    "ink": "^6.8.0",
    "pastel": "^3.0.0",
    "react": "^19.2.4",
    "viem": "2.23.15",
    "zod": "^3.25.76"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "tsconfig": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5"
  }
}
